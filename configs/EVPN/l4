        type ip-vrf
        admin-state enable
        interface ethernet-1/10.1 {
        }
        vxlan-interface vxlan1.1 {
        }
        protocols {
            bgp-evpn {
                bgp-instance 1 {
                    admin-state enable
                    vxlan-interface vxlan1.1
                    evi 10
                    ecmp 2
                }
            }
            bgp-vpn {
                bgp-instance 1 {
                    route-target {
                        export-rt target:1:10
                        import-rt target:1:10
                    }
                }
            }
        }
    }

